<div class="page-header">
  <h1>Paginas de Prueba</h1>
</div>


<table class="table table-bordered table-striped">
  <tr>
    <th>Id</th>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Acciones</th>
  </tr>

<% @pruebas.each do |prueba| %>
  <tr>
    <td><%= prueba.id %></td>
    <td><%= prueba.nombre %></td>
    <td><%= prueba.apellido %></td>
    <td>
      <% if can? :index, Prueba %>
          <%= link_to 'Show', prueba, :class => "btn" %>
      <% end %>

        <%= link_to 'Edit', edit_prueba_path(prueba), :class => "btn" %>
        <%= link_to 'Destroy', prueba, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn" %>
    </td>
  </tr>
<% end %>
</table>

<div class="form-actions">
  <%= link_to 'New Prueba', new_prueba_path, :class => "btn btn-primary" %>
</div>

